{
  "expo": {
    "name": "Spendiary",
    "slug": "spendiary",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F1FADA"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Esta app necesita acceso a tus fotos para guardar los reportes.",
        "NSPhotoLibraryAddUsageDescription": "Esta app necesita guardar archivos en tu galer√≠a."
      }
    },
    "android": {
      "newArchEnabled": true,
      "softwareKeyboardLayoutMode": "pan",
      "navigationBar": {
        "visible": "sticky-immersive"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F1FADA"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.armandoaranopla.spendiary"
    },
    "web": {
      "favicon": "./assets/favicon.ico"
    },
    "plugins": [
      "expo-localization",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "247a7e7b-c2d6-4b8f-baa1-5c60a4a7c14f"
      }
    }
  }
}
